{
  "name": "antd-color-replacer",
  "version": "1.4.0",
  "description": "antd  主题切换",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "ts:watch": "tsc -w -d",
    "build": "tsc -d",
    "prettier": "prettier --config .prettierrc --write \"./{src,tests}/**/*.{less,ts,tsx,js}\" \"!./{build.dist}/**\"",
    "webpack-dev": "cross-env NODE_ENV=development webpack-dev-server --inline  --config ./dev/webpack/config/webpack.config.dev.js",
    "webpack-build": "cross-env NODE_ENV=production webpack-cli --config ./dev/webpack/config/webpack.config.pro.js",
    "eslint": "eslint tests",
    "tslint": "tslint -p tsconfig.json",
    "lint-staged": "lint-staged"
  },
  "jest": {
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": "npm run tslint",
    "**/*.{js,jsx}": "npm run eslint",
    "**/*.{ts,tsx,js,jsx,d.ts}": [
      "npm run prettier",
      "git add"
    ],
    "*.{json,md,yml,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raohong/antd-color-replacer.git"
  },
  "keywords": [
    "antd",
    "postcss",
    "color"
  ],
  "author": "raohong",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "autoprefixer": "^9.6.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.12.0",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-babel": "^1.0.1",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "lint-staged": "^9.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@ant-design/colors": "^3.1.0",
    "color": "^3.1.2",
    "get-relative-luminance": "^0.3.1",
    "postcss": "^7.0.17"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "IE 11"
  ]
}
